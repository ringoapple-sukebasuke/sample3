

<h1>こんにちは</h1>
<%= form_with model: @company, url: search_path, method: :get, local: true do |form| %>
  <%= form.select :keyword, [ ['投稿が新しい順', 'new'],
                            ['投稿が古い順', 'old'],
                            ['いいねが多い順', 'likes'],
                            ['いいねが少ない順', 'dislikes'],
                            ['numberが高い順', 'number'],
                          ] %>
  <%= form.submit %>
<% end %>
